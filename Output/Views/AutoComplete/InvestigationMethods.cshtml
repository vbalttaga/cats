@using Weblib.Models
@using Weblib.Models.Common
@using LIB.Tools.BO
@using GofraLib.BusinessObjects
@{
    Layout = "~/Views/Master/_Js.cshtml";
}
@{
    this.Response.ContentType = "text/javascript";
}
var @(ViewData["Model"] + "_" + ViewData["Param"]) = [
@{var ind = 0;}
@foreach (var item in Model.Values)
{     
    @(ind!=0?", ":"") @:{value:@(((ItemBase)item).Id),label:"@(((ItemBase)item).GetName())",category: "@(((ItemBase)item).Category)" }
    ind++;
}            
];